{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "es2022",                                  
    "module": "esnext",                                
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,           
    "strict": true,
    "skipLibCheck": true                               
  }
}
